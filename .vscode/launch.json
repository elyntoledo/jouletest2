{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FruitSizeProfileCalculator",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}/FruitSizeProfileCalculator",
      "runtimeExecutable": "cds",
